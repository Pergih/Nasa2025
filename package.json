{
  "name": "stellareye",
  "version": "1.0.0",
  "description": "NASA Space Apps Challenge 2025 - Embiggen Your Eyes! Explore space through satellite perspectives",
  "scripts": {
    "dev": "./dev_stellareye.sh",
    "build": "./build_stellareye.sh",
    "start": "./run_stellareye.sh",
    "test": "./test_stellareye.sh",
    "build:prod": "./build_production.sh",
    "backend": "cd backend && uv run uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "frontend": "cd frontend && npm run dev",
    "install:backend": "cd backend && uv sync",
    "install:frontend": "cd frontend && npm install"
  },
  "keywords": [
    "nasa",
    "space",
    "astronomy",
    "satellite",
    "visualization",
    "react",
    "fastapi",
    "maps"
  ],
  "author": "StellarEye Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stellareye/stellareye"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}